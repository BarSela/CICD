<header>
  <link rel="stylesheet" href="/css/homePage.css" type="text/css" />
  <video id="myVideo" autoplay muted loop playsinline>
    <source
      src="/assets/homePageBackground.mp4"
      type="video/mp4"
      style="background-color: rgba(0, 0, 0, 0.75)"
    />
   </video>

  <div class="video-overlay">
  </div>
 
  <div class="navbar">
    <ul>
      <li><a id="home" class="active" href="/">Home</a></li>
      <li><a id="about" href="/aboutUs">About</a></li>
   
      <li><a id="login" href="/login" style="display: block">Login</a></li>
      <li><a id="logout" href="/logout" style="display: none; ">Logout</a></li>
    </ul>
   
    <ul><li><a id="logo-nav"> <h2 class="logo">SH<span>APE</span> </a></h2></li></ul>

  </div>

</header>
<script>
  
  if ("<%= userEmail %>" != "") {
    document.getElementById("login").style.display = "none";
    document.getElementById("logout").style.display = "block";
  } else {
    document.getElementById("login").style.display = "block";
    document.getElementById("logout").style.display = "none";
  }
</script>
