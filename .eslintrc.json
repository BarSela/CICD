{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"], // error when missing ";"
    "no-sparse-arrays": ["error"], //Disallows sparse arrays
    "no-unreachable": ["warn"],
    "no-unreachable-loop": ["warn"],
    "no-unsafe-negation": ["warn"],
    "consistent-return": ["warn"],
    "camelcase": ["error"]
  } //ignore err
}
